msgstr ""

#: ../../doc_src/cmds/test.rst:135
msgid "*NUM1* **-lt** *NUM2*"
msgstr ""

#: ../../doc_src/cmds/test.rst:136
msgid "Returns true if *NUM1* is less than *NUM2*."
msgstr ""

#: ../../doc_src/cmds/test.rst:138
msgid "*NUM1* **-le** *NUM2*"
msgstr ""

#: ../../doc_src/cmds/test.rst:139
