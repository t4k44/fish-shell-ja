msgstr ""

#: ../../doc_src/cmds/test.rst:149
msgid "*COND1* **-o** *COND2*"
msgstr ""

#: ../../doc_src/cmds/test.rst:150
msgid "Returns true if either *COND1* or *COND2* are true."
msgstr ""

#: ../../doc_src/cmds/test.rst:152
